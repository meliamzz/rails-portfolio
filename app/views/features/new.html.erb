<div class="new-feature">
  <div class="form col-10 col-sm-8 col-md-6 col-lg-4 mx-auto pt-5">
    <%= simple_form_for [ @project, @feature ], class: "pt-5" do |f| %>
    <div class="pt-5"><div class="pt-5">
    <h5>Nouvelle feature</h5>
      <%= f.input :description, label: "Description" %>
      <%= f.input :photo, as: :file %>
      <%= f.button :submit, "Ajouter" %>
    </div></div>
    <% end %>
  </div>
</div>
